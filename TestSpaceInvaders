import gym
env = gym.make("SpaceInvaders-v0")
observation = env.reset()
for _ in range(1000):
  env.render()
  action = env.action_space.sample()
  observation, reward, done, info = env.step(action)

  if done:
    observation = env.reset()
env.close()

#Does not require anything other than installing gym[all]
